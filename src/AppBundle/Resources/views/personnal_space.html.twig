                        <h3>Mon Profil</h3>
                        <div class="line"></div>
                        <div class="col-sm-4">
                            <p><strong>Civilité : </strong> {{ user_info[0].civility }}</p>
                            <p><strong>Prénom : </strong> {{ user_info[0].firstname }}</p>
                            <p><strong>Nom : </strong> {{ user_info[0].name }}</p>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Téléphone : </strong>{{ user_info[0].telephone }}</p>
                            <p><strong>Portable : </strong> {{ user_info[0].mobilePhone }}</p>
                        </div>
                        <div class="col-sm-4">
                            <p><strong>Adresse : </strong>{{ user_info[0].address }}</p>
                            <p><strong>CP & Ville : </strong>{{ user_info[0].postalCode }} {{ user_info[0].city }}</p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="line"></div>
                        <div class="col-sm-6">
                            <p><strong>Adresse email : </strong> {{ user_info[0].email }}</p>
                        </div>
